<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.testpro.websocketDataCommon.bean.WebsocketDataCommon">
	<resultMap type="WebsocketDataCommon" id="websocketDataCommonResultMap">
		<result property="id" column="id" />
		<result property="channelId" column="channel_id" />
		<result property="clientId" column="client_id" />
		<result property="courseId" column="course_id" />
		<result property="dFreq" column="d_freq" />
		<result property="fps" column="fps" />
		<result property="startTime" column="start_time" />
		<result property="endTime" column="end_time" />
		<result property="videoStartTimestamp" column="video_start_timestamp" />
		<result property="opType" column="op_type" />
		<result property="createdAt" column="created_at" />
		<result property="updatedAt" column="updated_at" />
		<result property="deletedAt" column="deleted_at" />
	</resultMap>
	
	<sql id="table_columns">
		id,
		channel_id,
		client_id,
		course_id,
		d_freq,
		fps,
		start_time,
		end_time,
		video_start_timestamp,
		op_type,
		created_at,
		updated_at,
		deleted_at
    </sql>
	<sql id="entity_properties">
		#{id},
		#{channelId},
		#{clientId},
		#{courseId},
		#{dFreq},
		#{fps},
		#{startTime},
		#{endTime},
		#{videoStartTimestamp},
		#{opType},
		#{createdAt},
		#{updatedAt},
		#{deletedAt}
	</sql>
	
	<!-- 适用于主键自增类型 -->
	<insert id="save" parameterType="WebsocketDataCommon" useGeneratedKeys="true" keyProperty="id">
		insert into websocket_data_common( <include refid="table_columns" /> ) 
		values ( <include refid="entity_properties" /> )
	</insert>
	
	<delete id="deleteByIds">
		delete from websocket_data_common
		where id in
		<foreach item="item" collection="array" open="(" separator="," close=")">
			#{item}
		</foreach>
	</delete>
	
	<update id="update" parameterType="WebsocketDataCommon">
		update websocket_data_common 
		<trim prefix="set" suffixOverrides=",">
			<if test="channelId != null and channelId != ''">channel_id = #{channelId},</if>
			<if test="clientId != null and clientId != ''">client_id = #{clientId},</if>
			<if test="courseId != null and courseId != ''">course_id = #{courseId},</if>
			<if test="dFreq != null and dFreq != ''">d_freq = #{dFreq},</if>
			<if test="fps != null and fps != ''">fps = #{fps},</if>
			<if test="startTime != null and startTime != ''">start_time = #{startTime},</if>
			<if test="endTime != null and endTime != ''">end_time = #{endTime},</if>
			<if test="videoStartTimestamp != null and videoStartTimestamp != ''">video_start_timestamp = #{videoStartTimestamp},</if>
			<if test="opType != null and opType != ''">op_type = #{opType},</if>
			<if test="createdAt != null and createdAt != ''">created_at = #{createdAt},</if>
			<if test="updatedAt != null and updatedAt != ''">updated_at = #{updatedAt},</if>
			<if test="deletedAt != null and deletedAt != ''">deleted_at = #{deletedAt},</if>
		</trim>
		<where>id = #{id}</where>
	</update>
	
	<select id="findById" resultMap="websocketDataCommonResultMap" parameterType="String" >
		select <include refid="table_columns" />
		from websocket_data_common
		where id = #{id}
	</select>
	
	<select id="queryAll" resultMap="websocketDataCommonResultMap">
		select <include refid="table_columns" /> 
		from websocket_data_common
	</select>
	
	<!-- 使用like用法：columnName like concat('%',#columnName#,'%') -->
	<sql id="page_where">
		<trim prefix="where" prefixOverrides="and | or ">
			<if test="channelId != null and channelId != ''">and channel_id = #{channelId}</if>
			<if test="clientId != null and clientId != ''">and client_id = #{clientId}</if>
			<if test="courseId != null and courseId != ''">and course_id = #{courseId}</if>
			<if test="dFreq != null and dFreq != ''">and d_freq = #{dFreq}</if>
			<if test="fps != null and fps != ''">and fps = #{fps}</if>
			<if test="startTime != null and startTime != ''">and start_time = #{startTime}</if>
			<if test="endTime != null and endTime != ''">and end_time = #{endTime}</if>
			<if test="videoStartTimestamp != null and videoStartTimestamp != ''">and video_start_timestamp = #{videoStartTimestamp}</if>
			<if test="opType != null and opType != ''">and op_type = #{opType}</if>
			<if test="createdAt != null and createdAt != ''">and created_at = #{createdAt}</if>
			<if test="updatedAt != null and updatedAt != ''">and updated_at = #{updatedAt}</if>
			<if test="deletedAt != null and deletedAt != ''">and deleted_at = #{deletedAt}</if>
		</trim>
	</sql>
	
	<select id="page" resultMap="websocketDataCommonResultMap" parameterType="WebsocketDataCommonSearch">
		select <include refid="table_columns" />
		from websocket_data_common
		<include refid="page_where" />
		limit #{firstIndex},#{pageSize}
	</select>
	
	<select id="pageCount" resultType="int" parameterType="WebsocketDataCommonSearch">
		select count(id) from websocket_data_common
		<include refid="page_where" />
	</select>
	
	<!-- 其他自定义SQL -->
	
</mapper>