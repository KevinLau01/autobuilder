<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.testpro.bizOrgIdMapping.bean.BizOrgIdMapping">
	<resultMap type="BizOrgIdMapping" id="bizOrgIdMappingResultMap">
		<result property="id" column="id" />
		<result property="orgId" column="org_id" />
		<result property="model" column="model" />
		<result property="bizId" column="biz_id" />
		<result property="sysId" column="sys_id" />
		<result property="bizData" column="biz_data" />
		<result property="deleted" column="deleted" />
		<result property="createdtime" column="createdtime" />
		<result property="createdUser" column="createdUser" />
		<result property="updatetime" column="updatetime" />
		<result property="updateUser" column="updateUser" />
	</resultMap>
	
	<sql id="table_columns">
		id,
		org_id,
		model,
		biz_id,
		sys_id,
		biz_data,
		deleted,
		createdtime,
		createdUser,
		updatetime,
		updateUser
    </sql>
	<sql id="entity_properties">
		#{id},
		#{orgId},
		#{model},
		#{bizId},
		#{sysId},
		#{bizData},
		#{deleted},
		#{createdtime},
		#{createdUser},
		#{updatetime},
		#{updateUser}
	</sql>
	
	<!-- 适用于主键自增类型 -->
	<insert id="save" parameterType="BizOrgIdMapping" useGeneratedKeys="true" keyProperty="id">
		insert into biz_org_id_mapping( <include refid="table_columns" /> ) 
		values ( <include refid="entity_properties" /> )
	</insert>
	
	<delete id="deleteByIds">
		delete from biz_org_id_mapping
		where id in
		<foreach item="item" collection="array" open="(" separator="," close=")">
			#{item}
		</foreach>
	</delete>
	
	<update id="update" parameterType="BizOrgIdMapping">
		update biz_org_id_mapping 
		<trim prefix="set" suffixOverrides=",">
			<if test="orgId != null and orgId != ''">org_id = #{orgId},</if>
			<if test="model != null and model != ''">model = #{model},</if>
			<if test="bizId != null and bizId != ''">biz_id = #{bizId},</if>
			<if test="sysId != null and sysId != ''">sys_id = #{sysId},</if>
			<if test="bizData != null and bizData != ''">biz_data = #{bizData},</if>
			<if test="deleted != null and deleted != ''">deleted = #{deleted},</if>
			<if test="createdtime != null and createdtime != ''">createdtime = #{createdtime},</if>
			<if test="createdUser != null and createdUser != ''">createdUser = #{createdUser},</if>
			<if test="updatetime != null and updatetime != ''">updatetime = #{updatetime},</if>
			<if test="updateUser != null and updateUser != ''">updateUser = #{updateUser},</if>
		</trim>
		<where>id = #{id}</where>
	</update>
	
	<select id="findById" resultMap="bizOrgIdMappingResultMap" parameterType="String" >
		select <include refid="table_columns" />
		from biz_org_id_mapping
		where id = #{id}
	</select>
	
	<select id="queryAll" resultMap="bizOrgIdMappingResultMap">
		select <include refid="table_columns" /> 
		from biz_org_id_mapping
	</select>
	
	<!-- 使用like用法：columnName like concat('%',#columnName#,'%') -->
	<sql id="page_where">
		<trim prefix="where" prefixOverrides="and | or ">
			<if test="orgId != null and orgId != ''">and org_id = #{orgId}</if>
			<if test="model != null and model != ''">and model = #{model}</if>
			<if test="bizId != null and bizId != ''">and biz_id = #{bizId}</if>
			<if test="sysId != null and sysId != ''">and sys_id = #{sysId}</if>
			<if test="bizData != null and bizData != ''">and biz_data = #{bizData}</if>
			<if test="deleted != null and deleted != ''">and deleted = #{deleted}</if>
			<if test="createdtime != null and createdtime != ''">and createdtime = #{createdtime}</if>
			<if test="createdUser != null and createdUser != ''">and createdUser = #{createdUser}</if>
			<if test="updatetime != null and updatetime != ''">and updatetime = #{updatetime}</if>
			<if test="updateUser != null and updateUser != ''">and updateUser = #{updateUser}</if>
		</trim>
	</sql>
	
	<select id="page" resultMap="bizOrgIdMappingResultMap" parameterType="BizOrgIdMappingSearch">
		select <include refid="table_columns" />
		from biz_org_id_mapping
		<include refid="page_where" />
		limit #{firstIndex},#{pageSize}
	</select>
	
	<select id="pageCount" resultType="int" parameterType="BizOrgIdMappingSearch">
		select count(id) from biz_org_id_mapping
		<include refid="page_where" />
	</select>
	
	<!-- 其他自定义SQL -->
	
</mapper>